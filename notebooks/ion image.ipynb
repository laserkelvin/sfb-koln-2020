   "source": [
    "function gaussian(x::AbstractFloat, μ::AbstractFloat, σ::AbstractFloat, A::AbstractFloat)\n",
    "    return A * exp(-(x - μ)^2 / (2 * σ^2))\n",
    "end\n",
    "\n",
    "\n",
    "function simulate_distributions()"
   ]
